{
  "name": "schrodinger-hat-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "vite build",
    "cy:o": "cypress open",
    "dev": "vite --host",
    "format": "prettier -w .",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "serve": "vite preview",
    "test:e2e": "vue-cli-service test:e2e",
    "test:unit": "vue-cli-service test:unit",
    "vue-build": "vue-tsc --noEmit && vite build",
    "vue-check": "vue-tsc --noEmit",
    "vue-dts": "vue-tsc --declaration --emitDeclarationOnly"
  },
  "dependencies": {
    "@vueuse/core": "^9.13.0",
    "cypress": "^12.8.1",
    "register-service-worker": "^1.7.2",
    "vue": "^3.2.45",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.1.6",
    "vue-tsc": "^1.2.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.34.1",
    "@types/node": "^18.11.18",
    "@vitejs/plugin-vue": "^4.0.0",
    "eslint": "^8.32.0",
    "prettier": "^2.5.1",
    "sass": "^1.49.9",
    "typescript": "^4.9.4",
    "vite": "^4.0.4"
  },
  "engines": {
    "node": ">=19.4.0",
    "npm": ">=9.2.0"
  }
}
